{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "SettingsOptions": {
    "PageSize": 5,
    "Link": "http://localhost:8080/"
  },
  "ServiceOptions": {
    "Seconds": 3
  },
  "Kafka":{
    "Connection" : {
      "SecurityProtocol" : "PLAINTEXT",
      "Host" : "localhost:8001"
    },
    "Consumer" : {
      "Topic" : "order_processing",
      "Group" : "1",
      "SizeBatch" : 1,
      "LimitWaitBuffer" : "00:01:00"
    },
    "Producer" : {
      "Topic" : "order_creation"
    }
  }
}
